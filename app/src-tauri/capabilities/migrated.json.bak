{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "context": "local",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "clipboard:write-all",
    "global-shortcut:allow-register",
    "window:allow-set-title",
    "window:allow-close",
    "window:allow-destroy",
    "dialog:allow-open",
    "dialog:allow-save",
    "image:allow-from-bytes",
    "image:allow-rgba",
    "fs:allow-home-read",
    "fs:allow-home-read-recursive",
    "fs:allow-home-meta",
    "fs:allow-home-meta-recursive",
    "fs:scope-home",
    "fs:scope-home-index",
    "fs:scope-home-recursive",
    "fs:allow-home-write",
    "fs:allow-home-write-recursive",
    "fs:allow-exists",
    "fs:read-all",
    {
      "identifier": "fs:allow-mkdir",
      "allow": [
        { "path": "$CONFIG/apicize" },
        { "path": "$HOME" },
        { "path": "$HOME/*" },
        { "path": "$HOME/**" },
        { "path": "$DOCUMENT/apicize" }
      ]
    },
    {
      "identifier": "fs:allow-copy-file",
      "allow": [
        { "path": "$RESOURCE/examples/*"},
        { "path": "$DOCUMENT/apicize" }
      ]
    }
  ],
  "platforms": [
    "linux",
    "macOS",
    "windows"
  ]
}