{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "context": "local",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "clipboard:write-all",
    "clipboard:monitor-all",
    "clipboard:read-all",
    "global-shortcut:allow-register",
    "core:window:allow-set-title",
    "core:window:allow-close",
    "core:window:allow-destroy",
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:default",
    "fs:allow-home-read",
    "fs:allow-home-read-recursive",
    "fs:allow-home-meta",
    "fs:allow-home-meta-recursive",
    {
      "identifier": "fs:scope",
      "allow": [{ "path": "$HOME" }, { "path": "$HOME/**" },{ "path": "$HOME/.ssh" }, { "path": "$HOME/.ssh/**" }]
    },
    "fs:allow-home-write",
    "fs:allow-home-write-recursive",
    "fs:allow-exists",
    "fs:read-all",
    "fs:allow-rename",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:allow-write-text-file",
    "fs:scope-download-recursive",
    "fs:scope-resource-recursive"
  ],
  "platforms": [
    "linux",
    "macOS",
    "windows"
  ]
}